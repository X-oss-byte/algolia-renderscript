{
  "name": "@algolia/renderscript",
  "version": "1.0.0",
  "description": "A custom JavaScript rendering engine based on puppeteer",
  "main": "dist/api/index.js",
  "scripts": {
    "start": "./scripts/test.sh",
    "dev": "yarn clean && ttsc && concurrently --kill-others \"ttsc -w --preserveWatchOutput\" \"PORT=${PORT:-3000} nodemon -w dist/\"",
    "build": "yarn clean && ttsc",
    "clean": "rm -rf dist/",
    "docker:install": "node dist/lib/install.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/algolia/renderscript.git"
  },
  "keywords": [
    "algolia",
    "puppeteer",
    "js renderer",
    "rendertron",
    "prerender"
  ],
  "author": "Algolia <contact@algolia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algolia/renderscript/issues"
  },
  "homepage": "https://github.com/algolia/renderscript#readme",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/puppeteer-core": "^1.9.0",
    "@types/uuid": "^3.4.4",
    "@types/yauzl-promise": "^2.1.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "ttypescript": "^1.5.6",
    "typescript": "^3.4.5",
    "typescript-transform-paths": "^1.1.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.0",
    "node-fetch": "^2.6.0",
    "puppeteer-core": "^1.17.0",
    "stream-to-string": "^1.2.0",
    "uuid": "^3.3.2",
    "yauzl-promise": "^2.1.3"
  }
}
